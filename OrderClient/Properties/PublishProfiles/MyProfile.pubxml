    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="MMDatabaseEntities" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=DELL-LATITUDE\SQLEXPRESS;Initial Catalog=MMDatabase;Persist Security Info=True;User ID=sa;Password=msqlserver;Application Name=EntityFramework" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\MMDatabaseEntities_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="metadata=res://*/OrderDatabase.csdl|res://*/OrderDatabase.ssdl|res://*/OrderDatabase.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=DELL-LATITUDE\SQLEXPRESS;initial catalog=MMDatabase;persist security info=True;user id=sa;Password=msqlserver;MultipleActiveResultSets=True;App=EntityFramework&quot;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
